<main>
    <article>
        <p>Create User</p>
        <ul>
            <li><label for "create-username">User Name: </label>
        <input type="text" name="create-username"></li>

            <li><label for "create-birthdate">Birthdate: </label>
        <input type="date" name="create-birthdate"></li>

            <li><label for "create-profile-message">Profile Message: </label>
        <input type="text" name="create-profile-message"></li>

        <li><button id="create-user">Create User</button></li>
        </ul>
    </article>

    <article>
        <p>Retrieve User</p>
        <ul>
            <li><label for "retrieve-username">User Name: </label>
        <input type="text" name="retrieve-username"></li>

            <li><label for "retrieve-birthdate">Birthdate: </label>
        <input type="date" name="retrieve-birthdate"></li>

        <li><button>Retrieve Users</button></li>
        </ul>

        <table class="output">
            <caption>Output - Leave fields blank to select rows without regard to the value of that column</caption>
            <tr>
                <th>User ID</th>
                <th>Username</th>
                <th>Birthdate</th>
                <th>Profile Message</th>
            </tr>
            <tr>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
            </tr>
        </table>
    </article>

    <article>
        <p>Update User</p>
        <ul>
            <li><label for "update-old-username">Old User Name: </label>
        <input type="text" name="update-old-username"></li>

            <li><label for "update-old-birthdate">Old Birthdate: </label>
        <input type="date" name="update-old-birthdate"></li>

            <li><label for "update-new-username">New User Name: </label>
        <input type="text" name="update-new-username"></li>

            <li><label for "update-new-birthdate">New Birthdate: </label>
        <input type="date" name="update-new-birthdate"></li>

            <li><label for "update-new-profile-message">New Profile Message: </label>
        <input type="text" name="update-new-profile-message"></li>

        <li><button>Update Users</button></li>
        </ul>
    </article>

    <article>
        <p>Delete User</p>
        <ul>
            <li><label for "delete-username">User Name: </label>
        <input type="text" name="delete-username"></li>

            <li><label for "delete-birthdate">Birthdate: </label>
        <input type="date" name="delete-birthdate"></li>

        <li><button>Delete User</button></li>
        </ul>
    </article>
</main>